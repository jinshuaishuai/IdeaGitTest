package rules;

import com.jin.drools.entity.ao.LevelAo;
import com.jin.drools.entity.dto.ResultDto;

rule "level-A"
    when
        $leveAo : LevelAo(level == 1);
        $resultDto : ResultDto();
    then
        $resultDto.setLevel("A");
        System.out.println("A 规则执行");
        System.out.println($resultDto);
     end
