<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jin.mapper.UserInfoMapper">
    <resultMap type="com.jin.entity.domain.UserInfoDO" id="BaseResultMap">
        <result column="id" jdbcType="INTEGER" property="id"/>
        <result column="name" jdbcType="VARCHAR" property="name"/>
        <result column="age" jdbcType="INTEGER" property="age"/>
        <result column="phone" jdbcType="VARCHAR" property="phone"/>
        <result column="is_delete" jdbcType="INTEGER" property="isDelete"/>
        <result column="create_time" jdbcType="VARCHAR" property="createTime"/>
        <result column="update_time" jdbcType="VARCHAR" property="updateTime"/>
    </resultMap>
    <sql id="Base_Columns">
        id,name,age,phone,is_delete,create_time,update_time
    </sql>
    <select id="getUserInfoById" resultMap="BaseResultMap" parameterType="int">
        select <include refid="Base_Columns"/> from user_info where id = #{id, jdbcType=INTEGER}
    </select>

    <select id="getDynamicQueryResult" resultType="java.util.LinkedHashMap">
        select <include refid="Base_Columns"/>
        from user_info
    </select>

